@model GymPFF.Models.RayosUV

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>RayosUV</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="RayosUVId" />
            <div class="form-group">
                Numero de cuarto
                <input asp-for="NumCuarto" class="form-control" />
                <span asp-validation-for="NumCuarto" class="text-danger"></span>
            </div>
            <div class="form-group">
                Hora de uso
                <input asp-for="HoraUso" class="form-control" />
                <span asp-validation-for="HoraUso" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regresar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
